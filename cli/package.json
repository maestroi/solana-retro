{
  "name": "@solana-retro/cli",
  "version": "1.0.0",
  "description": "CLI for publishing and managing on-chain cartridges",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "cartridge-cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:bundle": "esbuild src/index.ts --bundle --platform=node --target=node18 --outfile=dist/cartridge-cli.cjs --format=cjs",
    "build:binaries": "npm run build:bundle && pkg dist/cartridge-cli.cjs --targets node18-linux-x64,node18-linux-arm64,node18-macos-x64,node18-macos-arm64,node18-win-x64 --out-path dist/binaries",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@solana-retro/sdk": "file:../sdk",
    "@solana/web3.js": "^1.87.6",
    "bs58": "^5.0.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "esbuild": "^0.25.0",
    "pkg": "^5.8.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "files": [
    "dist"
  ],
  "pkg": {
    "scripts": "dist/cartridge-cli.cjs",
    "assets": []
  },
  "license": "MIT"
}

